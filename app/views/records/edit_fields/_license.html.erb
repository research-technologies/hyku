<% account_local_name =  ubiquity_url_parser(request.original_url) %>
<% tenant_work_settings = get_tenant_work_settings[account_local_name] %>
<% licence_list = tenant_work_settings.presence && tenant_work_settings['licence_list'].presence && tenant_work_settings['licence_list'].split(',') %>

<% license_service = Hyrax::LicenseService.new('licenses', licence_list) %>

<%= f.input :license, as: :multi_value_select,
    collection: license_service.selective_dropdown_display,
    include_blank: true,
    item_helper: license_service.method(:include_current_value),
    input_html: { class: 'form-control ubiquity-license' } %>
