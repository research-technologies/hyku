<% qualification_name_service = QualificationNameService.new %>
<% selected =  f.object[:qualification_name].empty? ? nil :  f.object[:qualification_name] %>
<% set_default = selected || 'PhD' %>

<% model = f.object.model.class.to_s.underscore %>

<%= render 'shared/ubiquity/works/qualification', f: f, model: model, qualification_service: qualification_name_service,
  set_default: set_default, field_name: 'qualification_name', css_class_name: 'qualification-name'%>
