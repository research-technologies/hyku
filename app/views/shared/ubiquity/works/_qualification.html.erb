<!-- Rendered from qualification_name.html.erb and qualification_level.jtml.erb-->

<% if model == 'generic_work' %>

  <%= f.input field_name.to_sym,
              collection: qualification_service.select_active_options,
              include_blank: true,
              input_html: { class: "form-control ubiquity-#{css_class_name}" } %>

<% else %>

  <%= f.input field_name.to_sym,
              collection: qualification_service.select_active_options,
              selected: set_default,
              include_blank: true,
              input_html: { class: "form-control ubiquity-#{css_class_name}" } %>

<% end %>
